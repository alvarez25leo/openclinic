  <%-- CONVERT TABLE --%>
  function convertTable(sText){
    aRows = sText.split("</TR>");
    sText = "";
    for (var i=0;i<aRows.length;i++){
      sReturn = "";
      sRow = aRows[i];
      aTds = sRow.split("</TD>");
      for (var y=0;y<aTds.length;y++){
        sTD = aTds[y];
        if((sTD.indexOf("delete")<0)&&(sTD.indexOf("<TD>")>-1)){
          sReturn += sTD+"</TD>";
        }
      }
      if(sReturn.length>0){
        sText+=("<TR>"+sReturn+"</TR>");
      }
    }
    return sText;
  }

